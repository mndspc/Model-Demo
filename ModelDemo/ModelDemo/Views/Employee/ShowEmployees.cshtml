@model ICollection<ModelDemo.Models.EmpMaster>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>List of Employees</title>
</head>
<body>
    <h3>Employees Details Are:</h3>
    <div> 
        <table>
            <tr>
                <th>Code</th>
                <th>Name</th>
                <th>Email</th>
            </tr>
            @foreach(var emp in Model)
            {
                <tr>
                    <td>@emp.EmpCode</td>
                    <td>@emp.EmpName</td>
                    <td>@emp.Email</td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
